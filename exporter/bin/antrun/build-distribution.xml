    <tarfileset dirmode="0755" filemode="0644" dir="bin">
      <exclude name="antrun/**"/>
      <exclude name="distribution/**"/>
      <exclude name="**/*.sh"/>
    </tarfileset>
    <tarfileset dirmode="0755" filemode="0755" dir="bin">
      <include name="**/*.sh"/>
      <include name="exporter"/>
    </tarfileset>
  </tar>
  <attachartifact file="bin/distribution/exporter-1.0-linux_amd64.tar.gz" classifier="linux_amd64" type="tar.gz"/>
</target>
</project>